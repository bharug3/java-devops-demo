pipeline {
    agent any
    stages {
        stage ('Testing') {
            steps {
                echo 'Testing.....'
            }
        }
        stage ('Sonar'){
            steps {
                echo 'Sonar....'
            }
        }
        stage ('publish') {
            steps {
                echo 'publishing...'
            }
        }
        stage ('deploy to dev') {
            steps {
                echo 'deploying to dev environment..'
            }
        }
        stage ('deploy to prod') {
            steps {
                echo 'deploy to prod......'
            }
        }

    }
}